<div class="container">		<table class="table">				<thead>				<tr>						<th scope="col"># Orden</th>						<th scope="col">Fecha Orden</th>						<th scope="col">Total</th>						<th scope="col">Estatus</th>				</tr>				</thead>				<tbody>								{{foreach ordenes}}				<tr>						<th scope="row">{{orderId}}</th>						<td>{{fechaOrden}}</td>						<td>{{total}}</td>						<td>{{estatus}}</td>				</tr>				{{endfor ordenes}}				<!-- Puedes agregar más filas aquí -->				</tbody>		</table></div>
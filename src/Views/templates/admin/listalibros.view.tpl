<div class="container-xl">		<div class="table-responsive">				<div class="table-wrapper">						<div class="table-title mb-5">								<div class="row">										<div class="col-sm-6 d-flex align-items-start">												<h2>Gestion de <b>Libros</b></h2>										</div>										<div class="col-sm-6 d-flex justify-content-end">												<a href="index.php?page=Admin_Form&mode=INS" class="btn rounded-3" >														<i class="material-icons">&#xE147;</i>														<span>Agregar Nuevo Libro</span>												</a>										</div>								</div>						</div>						<table class="table table-striped table-hover">								<thead>								<tr>										<th>ID</th>										<th>Libro</th>										<th>Autor</th>										<th>Precio</th>										<th>Stock</th>										<th>Categoria</th>										<th>Estatus</th>										<th>Actions</th>								</tr>								</thead>								<tbody>																{{foreach libros}}								<tr>										<td>{{libroId}}</td>										<td>{{libroNombre}}</td>										<td>{{libroAutor}}</td>										<td>{{libroPrecio}}</td>										<td>{{libroStock}}</td>										<td>{{categoria_nombre}}</td>										<td>{{libroStatus}}</td>										<td class="d-flex flex-row">												<a href="index.php?page=Admin_Form&mode=DSP&id={{libroId}}" class="display"><i 																class="material-icons" title="Ver">&#xE8F4;</i></a>												<a href="index.php?page=Admin_Form&mode=UPD&id={{libroId}}" class="edit"><i class="material-icons" title="Edit">&#xE254;</i></a>												<a href="index.php?page=Admin_Form&mode=DEL&id={{libroId}}" class="delete"><i class="material-icons" title="Delete">&#xE872;</i></a>										</td>								</tr>																{{endfor libros}}								</tbody>						</table>				</div>		</div></div>